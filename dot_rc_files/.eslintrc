/* Version: eslint@1.6.0
 * Reference:
 *  http://eslint.org/docs/user-guide/configuring.html
 *  http://eslint.org/docs/rules/
 * Tips:
 *  - Single line disable: //eslint-disable-line no-use-before-define
 *  - Global var disable: /[star] global someVar [star]/
 */
{
  "parser": "babel-eslint",
  "plugins": [
    "flow-vars"
  ],
  "rules": {
    "camelcase": 0,
    "quotes": 0,
    "no-console": 0,
    "no-use-before-define": 0,
    "no-underscore-dangle": 0,
    "dot-notation": 0,
    "semi": 2,
    "flow-vars/define-flow-type": 1,
    "flow-vars/use-flow-type": 1    
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": "eslint:recommended"
}
